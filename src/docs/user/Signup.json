            "github": {
                "type": "string",
                "description": "The github of the user",
                "example": "https://github.com/example",
                "default": "https://github.com/example",
                "required": true
            },
            "linkedin": {
                "type": "string",
                "description": "The linkedin of the user",
                "example": "https://www.linkedin.com/in/example",
                "default": "https://www.linkedin.com/in/example",
                "required": true
            },
