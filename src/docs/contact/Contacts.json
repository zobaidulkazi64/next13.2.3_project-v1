{
    "Contacts": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The id of the Contact", 
            "example": "507f1f77bcf86cd799439011",
            "default": "507f1f77bcf86cd799439011",
            "required": true,
            "readOnly": true
          },

          "full_name": {
            "type": "string",
            "description": "Name of the Contact",
            "example": "Zobaidul Kazi",
            "default": "Zobaidul Kazi",
            "required": true
          },

          "email": {
            "type": "string",
            "description": "Email of the Contact",
            "example": "zobaidul@ho.com",
            "default": "zobaidul@ho.com",
            "required": true,
            "format": "email",
            "pattern": "^([a-zA-Z0-9_\\-\\.]+)@([a-zA-Z0-9_\\-\\.]+)\\.([a-zA-Z]{2,5})$",
            "minLength": 1,
            "maxLength": 255,
            "lowercase": true

          },
          "company": {
            "type": "string",
            "description": "Company of the Contact",
            "example": "kazibyte",
            "default": "kazibyte",
            "required": true,
            "minLength": 1,
            "maxLength": 30
          },

          "message": {
            "type": "string",
            "description": "Message of the Contact",
            "example": "hello kazi, how are you",
            "default": "hello, His this problem",
            "required": true,
            "minLength": 1,
            "maxLength": 255
          },
          "created_at": {
            "type": "string",
            "description": "The created at of the blog",
            "example": "2022-01-01T00:00:00.000Z",
            "default": "2022-01-01T00:00:00.000Z",
            "required": true
          },
          "updated_at": {
            "type": "string",
            "description": "The updated at of the blog",
            "example": "2022-01-01T00:00:00.000Z",
            "default": "2022-01-01T00:00:00.000Z",
            "required": true
          },
          "deleted_at": {
            "type": "string",
            "description": "The deleted at of the blog",
            "example": "2022-01-01T00:00:00.000Z",
            "default": "2022-01-01T00:00:00.000Z",
            "required": true
          }
        }
    }
}